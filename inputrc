# these allow you to use ctrl+left/right arrow keys
# to jump the cursor over words
#"\e[1;5C": forward-word
#"\e[1;5D": backward-word
#"\e[5C": forward-word
#"\e[5D": backward-word
#"\e\e[C": forward-word
#"\e\e[D": backward-word

"\e[1;6D": backward-word
"\e[1;6C": forward-word

#set editing-mode vi
#set keymap vi
