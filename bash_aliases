alias ls='ls -G'

alias lsa='ls -laF'
alias ll='ls -l'

alias clean='mv $1 ~/clean/'
alias f='find . -name'

alias l='locate'

alias gcal='gcalcli'
alias rs='rsync -avz --partial --progress --rsh=ssh'

alias gs='git status'
alias gd='git diff'
alias ga='git add'
alias gc='git checkout'
alias screen='TERM=screen screen'
